        "import random \n",
        "N = 600000\n",
        "p = [0,0,0,0,0,0]\n",
        "for i in range(0,N) : \n",
        "  num = random.randint(1,6)\n",
        "  p[num-1]=p[num-1]+1\n",
        "print(p)\n",
        "pa=[]\n",
        "for ele in p : \n",
        "  pa.append(round(ele/N,3))\n",
        "print(pa)"
      ]
